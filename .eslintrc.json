{
    "extends": "airbnb",
    "plugins": [
        "jest",
        "react",
        "react-native"
    ],
    "parser": "babel-eslint",
    "env": {
        "jest/globals": true,
        "react-native/react-native": true
    },
    "rules": {
        "import/no-named-as-default": "off",
        "react/jsx-filename-extension": "off"
    }
}